    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "nconf": "^0.10.0",
    "node-cache": "^4.2.1",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "source-map-support": "^0.5.16"
